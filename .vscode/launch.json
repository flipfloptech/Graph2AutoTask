{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (Development)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Graph2AutoTask/bin/Debug/netcoreapp3.1/Graph2AutoTask.dll",
            "args": ["--app-settings-config-file","/opt/configs/appsettings.Development.json"],
            "cwd": "${workspaceFolder}/Graph2AutoTask",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (Debugging)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Graph2AutoTask/bin/Debug/netcoreapp3.1/Graph2AutoTask.dll",
            "args": ["--app-settings-config-file","/opt/configs/appsettings.Diagnostics.json"],
            "cwd": "${workspaceFolder}/Graph2AutoTask",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}